{
  "name": "LocAlarms",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "reverseIp": "adb reverse tcp:8081 tcp:8081"
  },
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "color": "^2.0.0",
    "idx": "^1.5.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "native-base": "^2.3.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-native": "^0.48.4",
    "react-native-android-location-services-dialog-box": "^2.2.1",
    "react-native-config": "^0.7.1",
    "react-native-datepicker": "^1.6.0",
    "react-native-maps": "^0.16.4",
    "react-native-push-notification": "^3.0.1",
    "react-native-sound": "^0.10.4",
    "react-native-vector-icons": "^4.4.0",
    "react-navigation": "^1.0.0-beta.12",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-form": "^7.0.4",
    "redux-persist": "^5.0.0-rc.2",
    "redux-saga": "^0.15.6",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-jest": "21.2.0",
    "babel-plugin-idx": "^2.0.0",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-preset-es2015": "^7.0.0-beta.2",
    "babel-preset-react-native": "^4.0.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "enzyme": "^3.0.0",
    "jest": "21.2.0",
    "react-addons-test-utils": "~16.0.0-alpha.3",
    "react-dom": "^16.0.0",
    "react-test-renderer": "16.0.0",
    "reactotron-react-native": "^1.12.3",
    "reactotron-redux": "^1.12.2",
    "reactotron-redux-saga": "^1.12.2"
  },
  "jest": {
    "testMatch": [
      "**/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "Tests/Setup.js"
    ],
    "setupFiles": [
      "./Tests/Setup"
    ],
    "preset": "react-native"
  },
  "config": {}
}
